TRITON_MODELS_GENERATION_BUILD_DIR=${TRITON_MODELS_GENERATION_BUILD_DIR:=/mnt/$CI_JOB_ID}

TRITON_MODELS_GENERATION_SRCDIR=${TRITON_MODELS_GENERATION_SRCDIR:=$TRITON_MODELS_GENERATION_BUILD_DIR/gen_srcdir}
